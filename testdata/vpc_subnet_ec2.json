{
  "metadata": {
    "version": "1.0",
    "name": "vpc-subnet-ec2",
    "description": "VPC with subnet and EC2",
    "environment": "production"
  },
  "nodes": [
    {
      "id": "node-vpc",
      "type": "vpc",
      "label": "Main VPC",
      "position": { "x": 200, "y": 100 },
      "properties": {
        "cidr_block": "10.0.0.0/16",
        "enable_dns_hostnames": true,
        "enable_dns_support": true
      }
    },
    {
      "id": "node-subnet",
      "type": "subnet",
      "label": "Public Subnet",
      "position": { "x": 200, "y": 200 },
      "properties": {
        "cidr_block": "10.0.1.0/24",
        "availability_zone": "us-east-1a"
      }
    },
    {
      "id": "node-ec2",
      "type": "ec2_instance",
      "label": "Web Server",
      "position": { "x": 200, "y": 300 },
      "properties": {
        "instance_type": "t3.micro",
        "ami": "ami-0c55b159cbfafe1f0",
        "key_name": "my-key",
        "tags": { "Name": "WebServer" }
      }
    }
  ],
  "edges": [
    { "id": "e1", "source": "node-vpc", "target": "node-subnet", "type": "contains" },
    { "id": "e2", "source": "node-subnet", "target": "node-ec2", "type": "contains" }
  ]
}
